<h2>My Interests</h2>
<ul class='my-interests'>
  <% @user.interests.each do |interest| %>
    <li id="interest_<%= interest.id %>">
    <% unless corresponding_skill(interest.name) == nil  %>
     (<%= link_to num_users_by_skills(interest.name), skill_path(corresponding_skill(interest.name)) %>)
      <% end %>
      <%= interest.name %>
       <%= link_to 'delete', user_interest_path(@user, interest), remote: true, :method => :delete, class: 'delete-interest', id: "interest-#{interest.id}", :data => { :id => "#{interest.id}"} %></li>
  <% end %>
</ul>